                    "type": "FunctionDefinition",
                    "name": "function()externalpayable{require(msg.value>=0);seed.transfer(msg.value/(daily_percent*2));uintblock_timestamp=now;if(balances[msg.sender]!=0){uint256pay_out=balances[msg.sender]*daily_percent/100*(block_timestamp-timestamps[msg.sender])/86400;if(address(this).balance<pay_out)pay_out=address(this).balance;msg.sender.transfer(pay_out);emitPayout(msg.sender,pay_out);}timestamps[msg.sender]=block_timestamp;balances[msg.sender]+=msg.value;if(msg.value>0)emitAcountTopup(msg.sender,balances[msg.sender]);}",

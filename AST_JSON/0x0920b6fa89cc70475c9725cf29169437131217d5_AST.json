                {
                    "type": "FunctionDefinition",
                    "name": "function()payablepublicwithinRange{if(punterWaiting){uint256_payout=min(msg.value,puntAmount);if(rand(punterAddress)>=rand(msg.sender)){punterAddress.transfer(_payout+puntAmount);if((msg.value-_payout)>0)msg.sender.transfer(msg.value-_payout);}else{msg.sender.transfer(_payout+msg.value);if((puntAmount-_payout)>0)punterAddress.transfer(puntAmount-_payout);}punterWaiting=false;}else{punterWaiting=true;punterAddress=msg.sender;puntAmount=msg.value;}}",
